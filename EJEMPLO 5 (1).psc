Proceso EJEMPLO05
	//solicitar 2 números y mostrar la suma de ambos
	
	Definir A,B,Resultado Como Real;
	
	Escribir "asignar un número A";
	Leer A;
	Escribir "Asignar un número B";
	Leer B;
	
	Resultado <- B + A;
	
	//SALIDA
	
	Escribir "El resultado de la suma de ", A , " + ", B , " es: " , Resultado;
	
	
FinProceso
